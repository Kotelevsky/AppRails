<h1>Автомобили</h1>

<%= form_for Auto.new do |f| %>
<div class="field">
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.submit "Add" %>
</div>
<% end %>

<table>
<tr>
  <th>id</th>
  <th>Название</th>
  <th></th>
</tr>
<% @auto.each do |f| %>
<tr>
  <td><%= f.id %></td>
  <td><%= f.name %></td>
  <td><%= link_to "Edit", edit_auto_path(f) %></td>
</tr>
<% end %>
</table>
